{
  "name": "cryptopi-dynamics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc -p .",
    "format": "prettier --write .",
    "check": "npm run lint && npm run typecheck",
    "check:all": "npm run check && npm run smoke:prod",
    "db:clean": "node -e \"const {Client}=require('pg');(async()=>{const c=new Client({connectionString:process.env.DATABASE_URL});await c.connect();const s=process.env.DB_SCHEMA||'cryptopi';console.log('[db:clean] dropping schema',s);await c.query(`DROP SCHEMA IF EXISTS \\\"${process.env.DB_SCHEMA||'cryptopi'}\\\" CASCADE;`);await c.end()})().catch(e=>{console.error(e);process.exit(1)})\"",
    "db:reset": "npm run db:clean && npm run db:ddl",
    "db:migrate-schema": "node --env-file=.env ./src/scripts/db/migrate-to-schema.mjs",
    "smoke:apis:current": "node src/scripts/smokes/smoke-apis-current.mjs",
    "db:whereami": "node --env-file=.env ./src/scripts/db/whereami.mjs",
    "smoke:db-saves": "node src/scripts/smokes/smoke-db-saves.mjs",
    "doctor": "node src/scripts/smokes/doctor.mjs",
    "db:ddl": "node --env-file=.env src/scripts/db/run-ddl.cjs",
    "smoke:ui": "node src/scripts/smokes/smoke-ui.mjs",
    "smoke:features": "node src/scripts/smokes/smoke-features.mjs",
    "xray": "node src/scripts/smokes/xray.mjs",
    "adapter": "node src/scripts/smokes/adapter.mjs",
    "one-tick": "node src/scripts/smokes/onetick.mjs",
    "db:upsert": "node --env-file=.env src/scripts/db-upsert.ts",
    "orchestrator": "node src/scripts/orchestrator/orchestrator.ts"

  },
  "dependencies": {
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "webpack": "^5.101.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
